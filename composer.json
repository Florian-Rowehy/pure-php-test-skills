{
    "name": "whatafix/text-tagger",
    "description": "Libray that tag text according to it's content",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.4"
    },
    "autoload": {
        "psr-4": {
            "Whatafix\\TextTagger\\": "src/",
            "Whatafix\\TextTagger\\Test\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "phpstan/phpstan": "^0.12.55",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload-dev": {
        "psr-4": {
            "Whatafix\\TextTagger\\Test\\": "tests/"
        }
    }
}
